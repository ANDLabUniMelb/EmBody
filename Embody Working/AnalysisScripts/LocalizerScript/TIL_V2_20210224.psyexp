    <Routine name="Introduction">
      <CodeComponent name="Intro_code">
        <Param name="Before Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="# Import&amp;#10;import pyglet&amp;#10;import key&amp;#10;import math&amp;#10;&amp;#10;# make subject data folder - &gt; Remove It&amp;#10;subject_name = str(expInfo[u'participant'])&amp;#10;subject_folder = 'data/'+subject_name&amp;#10;os.mkdir(subject_folder)&amp;#10;&amp;#10;# Make Keyhandler for movement&amp;#10;keyState=key.KeyStateHandler()&amp;#10;win.winHandle.push_handlers(keyState)&amp;#10;&amp;#10;# Basic setup&amp;#10;p1size = 0.05 # P1 size&amp;#10;p2size = 0.05 # P2 size&amp;#10;p1v = 0.007&amp;#10;p2v0_as_predator = 0.005&amp;#10;p2v0_as_prey = 0.007&amp;#10;p2v1_as_predator = p2v0_as_predator&amp;#10;p2v1_as_prey = p2v0_as_prey&amp;#10;alpha=0.2&amp;#10;# Trial Over Sentence Duration&amp;#10;TO_Dur = 0&amp;#10;# Color&amp;#10;p1color = [-1,1,-1]&amp;#10;p2color = [1,-1,-1]&amp;#10;&amp;#10;# P2 Boost information&amp;#10;p2boost = 0&amp;#10;boost_count = 0&amp;#10;Boost_Start0 = 120&amp;#10;Boost_spd0 = 1&amp;#10;Boost_spd1 = Boost_spd0&amp;#10;boost_count_off = 30&amp;#10;# Difficulty Calibration&amp;#10;Boost_Inc = 1.2&amp;#10;Boost_Dec = (1/1.2)&amp;#10;p2v_Inc = 1.2&amp;#10;p2v_Dec = (1/1.2)&amp;#10;p2v_Inc_as_predator = 1.05&amp;#10;p2v_Dec_as_predator = (1/1.05)&amp;#10;&amp;#10;# Trial Count&amp;#10;TrialNum = 0 # Trial number initialization&amp;#10;PreyTrialNum = 0&amp;#10;PreyTrialSuccess = 0&amp;#10;PredatorTrialNum = 0&amp;#10;PredatorTrialSuccess = 0&amp;#10;&amp;#10;# N of Trials, length&amp;#10;Tlength = 6 # Trial length&amp;#10;Ntrial = 101 # 4*N + 1&amp;#10;# Difficulty calibration trials&amp;#10;Check_Freq = 5&amp;#10;Check_Start = 2&amp;#10;Check_Trial=np.linspace(Check_Freq*Check_Start,Ntrial-1-Check_Freq,int((Ntrial-1)/Check_Freq)-Check_Start)&amp;#10;&amp;#10;# Trial condition setting&amp;#10;# 1 Switch/Stay Duration&amp;#10;LDur = [2, 2] #[1.75, 2.25] # Long Switch/Stay duration mean (s)&amp;#10;SDur = [0.5, 0.5] #[0.25, 0.75] # Short Switch/Stay duration  mean (s)&amp;#10;# 2 Distance&amp;#10;FarDist = [0.6,0.7] # Far distance mean&amp;#10;NearDist= [0.2,0.3] # Near distance mean&amp;#10;Nswitch = Ntrial-1&amp;#10;Nhalf = int(Nswitch/2)&amp;#10;# 3 Switch/Stay arrange - Switch=1 Stay=0&amp;#10;SwitchStay = np.zeros(Ntrial) &amp;#10;Perm_Trial = np.random.permutation(Nswitch)&amp;#10;Switch_Trial = Perm_Trial[0:Nhalf]&amp;#10;Stay_Trial = Perm_Trial[Nhalf:Nswitch]&amp;#10;SwitchStay[Switch_Trial] = 1&amp;#10;# 4 Role arange: Predator role=1 or Prey role=0&amp;#10;Role = np.zeros(Ntrial)&amp;#10;Role[0] = np.round(np.random.uniform(0,1))&amp;#10;for i in range(Nswitch):&amp;#10;    if SwitchStay[i]==1:&amp;#10;        Role[i+1]=1-Role[i]&amp;#10;    elif SwitchStay[i]==0:&amp;#10;        Role[i+1]=Role[i]&amp;#10;        &amp;#10;# Define Switch Duration, Final switch is ending sentence&amp;#10;SwitchDur=np.zeros(Ntrial) # Final is end of Session&amp;#10;LongSwitch = Switch_Trial[0:int(Nhalf/2)]&amp;#10;ShortSwitch = Switch_Trial[int(Nhalf/2):Nhalf]&amp;#10;LongStay = Stay_Trial[0:int(Nhalf/2)]&amp;#10;ShortStay = Stay_Trial[int(Nhalf/2):Nhalf]&amp;#10;LongShortSwitchStayInd=np.array((LongSwitch,ShortSwitch,LongStay,ShortStay))&amp;#10;LongShortSwitchStayInd=np.transpose(LongShortSwitchStayInd)&amp;#10;SwitchDur[LongSwitch]=np.random.uniform(LDur[0],LDur[1],len(LongSwitch))&amp;#10;SwitchDur[LongStay]=np.random.uniform(LDur[0],LDur[1],len(LongStay))&amp;#10;SwitchDur[ShortSwitch]=np.random.uniform(SDur[0],SDur[1],len(ShortSwitch))&amp;#10;SwitchDur[ShortStay]=np.random.uniform(SDur[0],SDur[1],len(ShortStay))&amp;#10;SwitchDur[Nswitch]=5 # End of the session&amp;#10; &amp;#10;# Distance: Far = 1 Near = 0 trial&amp;#10;FarNear = np.zeros(Ntrial) &amp;#10;FarNear[0] = np.round(np.random.uniform(0,1))&amp;#10;Perm_Trial_FN = 1+np.random.permutation(Nswitch)&amp;#10;FarTrial = Perm_Trial_FN [0:int(Nswitch/2)]&amp;#10;FarNear[FarTrial] = 1&amp;#10;TrialDist= np.zeros(Ntrial) &amp;#10;FarD=np.random.uniform(FarDist[0],FarDist[1],len(np.argwhere(FarNear==1)))&amp;#10;FarD=FarD.reshape(len(np.argwhere(FarNear==1)),1)&amp;#10;NearD=np.random.uniform(NearDist[0],NearDist[1],len(np.argwhere(FarNear==0)))&amp;#10;NearD=NearD.reshape(len(np.argwhere(FarNear==0)),1)&amp;#10;TrialDist= np.zeros(Ntrial) &amp;#10;TrialDist[np.argwhere(FarNear==1)]=FarD&amp;#10;TrialDist[np.argwhere(FarNear==0)]=NearD&amp;#10;&amp;#10;# Initial Position of P1 and P2&amp;#10;P1theta = np.random.uniform(0,2*np.pi,Ntrial)&amp;#10;P1pos=np.array((np.multiply(np.cos(P1theta),TrialDist/2),np.multiply(np.sin(P1theta),TrialDist/2)))&amp;#10;P1pos=np.transpose(P1pos)&amp;#10;P2pos = -P1pos&amp;#10;&amp;#10; # Fixation initialization&amp;#10;FixList_StoP=np.random.uniform(0.5,1.5,Ntrial)&amp;#10;FixList_PtoS=np.random.uniform(0.5,1.5,Ntrial)&amp;#10;Fixlist = np.array((FixList_StoP,FixList_PtoS))&amp;#10;&amp;#10;# Save initial setting of each trial&amp;#10;np.savetxt(subject_folder+'/Role.txt',Role)&amp;#10;np.savetxt(subject_folder+'/SwitchStay.txt',SwitchStay[0:Nswitch])&amp;#10;np.savetxt(subject_folder+'/LongShortSwitchStayInd.txt',LongShortSwitchStayInd)&amp;#10;np.savetxt(subject_folder+'/SwitchStayDur.txt',SwitchDur[0:Nswitch])&amp;#10;np.savetxt(subject_folder+'/Far1Near0.txt',FarNear)&amp;#10;np.savetxt(subject_folder+'/TrialDist.txt',TrialDist)&amp;#10;np.savetxt(subject_folder+'/P1pos.txt',P1pos)&amp;#10;np.savetxt(subject_folder+'/P2pos.txt',P2pos)&amp;#10;np.savetxt(subject_folder+'/Fixlist.txt',Fixlist)&amp;#10;np.savetxt(subject_folder+'/Role.txt',Role)" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="i_image = 'I1.png';&amp;#10;inst_page = 1;&amp;#10;move_inst = '';" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="Instruction_Image.setImage(i_image);" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="if Role[0]==1:&amp;#10;    Init_Behav=&quot;Attack&quot;&amp;#10;elif Role[0]==0:&amp;#10;    Init_Behav=&quot;Escape&quot;&amp;#10;    &amp;#10; # P1 information&amp;#10;p1xINIT = P1pos[TrialNum,0]&amp;#10;p1yINIT =  P1pos[TrialNum,1]&amp;#10;# P2 information&amp;#10;p2xINIT = P2pos[TrialNum,0]&amp;#10;p2yINIT = P2pos[TrialNum,1]   " valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="JS" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="let inst_move_keys = psychoJS.eventManager.getKeys();&amp;#10;if (inst_move_keys.length &gt; 0) {  // at least one key was pressed&amp;#10;  move_inst = inst_move_keys[inst_move_keys.length-1]; &amp;#10;  }&amp;#10;&amp;#10;if ((inst_page&gt;1)&amp;&amp;(move_inst=='left')) {&amp;#10;    inst_page -= 1;&amp;#10;    i_image = 'I'+inst_page+'.png';&amp;#10;    Instruction_Image.setImage(i_image);&amp;#10;    }&amp;#10;if (move_inst=='right') {&amp;#10;    inst_page += 1;&amp;#10;    if (inst_page==7) {&amp;#10;        continueRoutine = false;&amp;#10;        } else {&amp;#10;            i_image = 'I'+inst_page+'.png';&amp;#10;            Instruction_Image.setImage(i_image);&amp;#10;            }   &amp;#10;    }&amp;#10;move_inst = '';" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="&amp;#10;xz" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
